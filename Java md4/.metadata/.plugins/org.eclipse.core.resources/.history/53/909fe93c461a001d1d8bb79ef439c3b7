package Strings_Progms;
//WAP yo print alternate character's from string
// IP:java development
// OP:jv eeoe

public class Progm_2 {

	public static void main(String[] args) {
     String s1=new String("Java Development");//16
     int len=s1.length();
     for(int i=0;i<len;i=i+2)
     {
    	 char ch=s1.charAt(i);
    	 System.out.print(ch);
     }
	}

}
