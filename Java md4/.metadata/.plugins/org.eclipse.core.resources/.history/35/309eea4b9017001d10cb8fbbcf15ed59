package Arrays_Practise;

import java.util.Scanner;

public class Array_12_2 {

	public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int size=sc.nextInt();
    int a[]=new int[size];
    for(int i=0;i<size;i++)
    {
    	a[i]=sc.nextInt();
    }
    int max=0,min=0;
    for(int i=0;i<a.length;i++)
    {
    	max=a[i];
    	min=a[i];
    }
    
   int diff=max-min;
	}

}
