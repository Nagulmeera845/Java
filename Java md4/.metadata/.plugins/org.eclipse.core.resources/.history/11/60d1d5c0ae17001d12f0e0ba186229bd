package Arrays_Practise;

import java.util.Scanner;

public class Array_7_1_OddOf_Indexnums {

	public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter your array size is:");
    int size=sc.nextInt(); //scanner method 
    System.out.println("Enter your lucky num's in array:");
   	int n[]=new int[size];
   	for(int i=0;i<size;i++) //logic for size/length 
   	{
   	  n[i]=sc.nextInt();
   	}
   	System.out.println("Odd num's from array is:");
   	for(int j=0;j<size;j++)
   	{
   		if(j%2!=0) //for oddindexposition num's
   		System.out.println(n[j]);//For printing
   		sc.close();
   	}
    
	}

}
