package Day2;

public class SwapCommandLineArgs {

	public static void main(String[] args) {
     int a=Integer.parseInt(args[0]);
     int b=Integer.parseInt(args[1]);
     System.out.println("Non swapped : "+" "+ " ");
     a=a+b;
     b=a-b;
     a=a-b;
     System.out.println("swapped : "+a+" "+b);
	}

}
