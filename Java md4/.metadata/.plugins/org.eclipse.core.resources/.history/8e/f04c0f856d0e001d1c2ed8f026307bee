package Day3;

public class Recursion
{

	public static void rec(int i)
	{
		if(i<=100)
		{
			int num=i,rem,arm=0,temp=num;
			while(num>0)
			{
				rem=num%10;
				num=num/10;
				arm=arm+rem*rem*rem;
			}
			if(temp==arm) {
				System.out.println("arm"+arm);
			}
			if(i<=1000) {
			System.out.println(i);
		i=i+1;
		rec(i);
     }
	
 public static void main(String args[]) {
	rec(1); 
}
}