package Important_Programs;

public class Printonly_Even_values {

	public static void main(String[] args) {
    int arr[]=new int[] {15,6,74,16,18,29};
    System.out.print("Reverse of array values: ");
    for(int i=arr.length-1;i>=0;i--)
    {
    	if(i%2==0)
    	System.out.print(arr[i]+" ");
    }
	}
  }
