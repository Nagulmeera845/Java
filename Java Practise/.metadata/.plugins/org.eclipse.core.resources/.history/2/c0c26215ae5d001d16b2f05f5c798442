package Local_Global;

public class Static_NonStatic_Mem {
	
	public void nagul() {
		String name="Nagul is intelligent";
	}
	int a=15;
  static int b=20;
  
  public static void main(String[] args) {
	  Static_NonStatic_Mem obj1 =new Static_NonStatic_Mem();
	  System.out.println(obj1.a);
	  Static_NonStatic_Mem obj2 =new Static_NonStatic_Mem();
	 System.out.println(obj2.nagul());
	System.out.println(b);
	  
}
}
