package Local_Global;

public class Static_NonStatic_Mem {
	int a=15;
	public void nagul() {
		String name="Nagul is intelligent";
	}
  static int b=20;
  
  public static void main(String[] args) {
	  Static_NonStatic_Mem obj1 =new Static_NonStatic_Mem();
	  System.out.println(obj1.a);
	 System.out.println(obj1.name);
	System.out.println(b);
	  
}
}
