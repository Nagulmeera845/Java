package Gaswani;
class Gmail{
	private String uname,pwd;
	// setter for uname
	public void setUname(String uname) {
		this.uname=uname;
	}
	public void setPwd(String pwd) {
		this.pwd=pwd;
	}
	// getter for uname
	public String getUname() {
		// validate
		if(uname=="guru99@gmail.com") {
			return  "valid gamil address";
		}
		else {
			return " Invalid gmail,please enter correct address";
		}
		
	}
	// getter dor pwd
	public String getPwd(){
		if(pwd=="guru99") {
			return "valid passwaord";
		}
		else {
			return "invalid passwaord ,enter correct password";
		}
	}
}

public class Gmail {

	public static void main(String[] args) {
		Gmail g1=new Gmail();
		g1.setUname("nagul@gmail.com");
		g1.setPwd("123san");
		
		System.out.println(g1.getUname);
		System.out.println(g1.getPwd);
		

	}

}
