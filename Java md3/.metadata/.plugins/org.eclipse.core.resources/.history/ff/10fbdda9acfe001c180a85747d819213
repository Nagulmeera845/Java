package Pattern_Programs;

public class Prog_6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int row=1;row<=5;row++) {
			for(int col=1;col<=5;col++) {
				if(row==3 ||col==3 )
					System.out.print("*");
				else
					System.out.println(" ");
			}
			System.out.println();
		}

	}

}
